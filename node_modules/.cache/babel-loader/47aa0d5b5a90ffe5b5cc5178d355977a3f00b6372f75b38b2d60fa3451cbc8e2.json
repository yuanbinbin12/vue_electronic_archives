{"ast":null,"code":"import { mapMutations } from 'vuex';\nimport $ from \"jquery\";\nexport default {\n  name: 'WarningAlter',\n  data() {\n    return {\n      scrollX: 0,\n      scrollY: 0\n    };\n  },\n  props: [\"warningMsg\", \"typeMsg\"],\n  methods: {\n    ...mapMutations(\"grounding\", [\"setWarningMsg\"]),\n    handleScroll() {\n      this.$nextTick(() => {\n        var top = document.body.scrollTop || document.documentElement.scrollTop || window.pageYOffset;\n        var left = document.body.scrollLeft || document.documentElement.scrollLeft || window.pageXOffset;\n        this.scrollX = top;\n        this.scrollY = left;\n      });\n    }\n  },\n  mounted() {\n    //可以在这里面直接进行滚动条的获取\n    window.addEventListener('scroll', this.handleScroll, true);\n  },\n  watch: {\n    warningMsg(newValue, oldValue) {\n      if (newValue !== '') {\n        $(html).css({\n          \"overflow\": \"hidden\"\n        });\n        $(\".Masking-layer\").css({\n          left: this.scrollY,\n          top: this.scrollX\n        });\n      } else {\n        $(html).css({\n          \"overflow\": \"visible\"\n        });\n      }\n    }\n  }\n};","map":{"version":3,"names":["mapMutations","$","name","data","scrollX","scrollY","props","methods","handleScroll","$nextTick","top","document","body","scrollTop","documentElement","window","pageYOffset","left","scrollLeft","pageXOffset","mounted","addEventListener","watch","warningMsg","newValue","oldValue","html","css"],"sources":["src/components/WarningAlter.vue"],"sourcesContent":["<template>\r\n    <div v-show=\"warningMsg!==''\" class=\"Masking-layer\" >\r\n        <div v-if=\"warningMsg!==''\" style=\"position: absolute; left: 50%; top: 50%; transform: translate(-50%,-50%);\">\r\n            <el-alert\r\n                @close=\"setWarningMsg({'message':'','type':''})\"\r\n                :title=\"typeMsg==='error'?'':''\"\r\n                :type=\"typeMsg\"\r\n                :description=\"warningMsg\"\r\n                show-icon>\r\n            </el-alert>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport {mapMutations} from 'vuex';\r\nimport $ from \"jquery\";\r\nexport default {\r\n    name: 'WarningAlter',\r\n    data(){\r\n        return{\r\n            scrollX:0,\r\n            scrollY:0\r\n        }\r\n    },\r\n    props:[\"warningMsg\",\"typeMsg\"],\r\n    methods:{\r\n        ...mapMutations(\"grounding\",[\"setWarningMsg\"]),\r\n        handleScroll() {\r\n        this.$nextTick(()=>{\r\n                var top = document.body.scrollTop || document.documentElement.scrollTop || window.pageYOffset;\r\n                var left = document.body.scrollLeft || document.documentElement.scrollLeft ||window.pageXOffset;\r\n                this.scrollX = top;\r\n                this.scrollY = left;\r\n            })\r\n        }\r\n    },\r\n    mounted() {//可以在这里面直接进行滚动条的获取\r\n\twindow.addEventListener('scroll', this.handleScroll, true)\r\n    },\r\n    watch:{\r\n        warningMsg(newValue,oldValue){\r\n            if(newValue !==''){\r\n                $(html).css({\r\n                    \"overflow\": \"hidden\"\r\n                });\r\n                $(\".Masking-layer\").css({\r\n                    left:this.scrollY,\r\n                    top:this.scrollX\r\n                })\r\n            } else{\r\n                $(html).css({\r\n                    \"overflow\": \"visible\"\r\n                });\r\n            }\r\n        }\r\n    }\r\n};\r\n</script>\r\n\r\n<style lang=\"css\" >\r\n.el-alert__closebtn{\r\n    font-size: 30px !important;\r\n}\r\n.el-alert__description{\r\n    font-size: 22px !important; \r\n    margin-left: 40px !important;\r\n    margin-right: 40px !important;\r\n    margin-bottom: 30px !important;\r\n}\r\n.el-alert__title{\r\n    font-size: 26px;\r\n}\r\n.el-alert__icon{\r\n    position: relative;\r\n    top:5px;\r\n    left: 40px;\r\n}\r\n.el-alert__content{\r\n    padding-top: 30px;\r\n}\r\n.Masking-layer{\r\n    position: absolute;\r\n    top:0;\r\n    bottom: 0;\r\n    left: 0;\r\n    right: 0;\r\n    background-color: rgba(0, 0, 0, 0.5);\r\n}\r\n</style>"],"mappings":"AAeA,SAAAA,YAAA;AACA,OAAAC,CAAA;AACA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,OAAA;MACAC,OAAA;IACA;EACA;EACAC,KAAA;EACAC,OAAA;IACA,GAAAP,YAAA;IACAQ,aAAA;MACA,KAAAC,SAAA;QACA,IAAAC,GAAA,GAAAC,QAAA,CAAAC,IAAA,CAAAC,SAAA,IAAAF,QAAA,CAAAG,eAAA,CAAAD,SAAA,IAAAE,MAAA,CAAAC,WAAA;QACA,IAAAC,IAAA,GAAAN,QAAA,CAAAC,IAAA,CAAAM,UAAA,IAAAP,QAAA,CAAAG,eAAA,CAAAI,UAAA,IAAAH,MAAA,CAAAI,WAAA;QACA,KAAAf,OAAA,GAAAM,GAAA;QACA,KAAAL,OAAA,GAAAY,IAAA;MACA;IACA;EACA;EACAG,QAAA;IAAA;IACAL,MAAA,CAAAM,gBAAA,gBAAAb,YAAA;EACA;EACAc,KAAA;IACAC,WAAAC,QAAA,EAAAC,QAAA;MACA,IAAAD,QAAA;QACAvB,CAAA,CAAAyB,IAAA,EAAAC,GAAA;UACA;QACA;QACA1B,CAAA,mBAAA0B,GAAA;UACAV,IAAA,OAAAZ,OAAA;UACAK,GAAA,OAAAN;QACA;MACA;QACAH,CAAA,CAAAyB,IAAA,EAAAC,GAAA;UACA;QACA;MACA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}